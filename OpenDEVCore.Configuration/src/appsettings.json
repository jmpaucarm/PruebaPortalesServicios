{
  "app": {
    "name": "configuration-api"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=xxxxxxxx;Database=CONFIGURATION_SC;Trusted_Connection=False;User ID=xxxxxxxx;Password=xxxxxxxx"
